    "prettier": "^2.7.1",
    "serverless": "^3.15.0",
    "serverless-offline": "^11.0.1",
    "serverless-webpack": "^5.10.0",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.4.0",
